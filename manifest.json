{
    "name": "Doggos",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Doggos are the new todos!",
    "browser_action": {
        "default_title": "Doggos",
        "default_popup": "popup.html"
    },
    "content_scripts": [ 
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
        
}